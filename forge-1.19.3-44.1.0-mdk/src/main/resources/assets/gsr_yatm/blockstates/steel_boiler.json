{
	"multipart": [
		{
			"when": {
				"AND": [
					{
						"lit": "false",
						"facing": "north"
					}
				]
			},
			"apply": {
				"model": "gsr_yatm:block/steel_boiler"
			}
		},
		{
			"when": {
				"AND": [
					{
						"lit": "false",
						"facing": "east"
					}
				]
			},
			"apply": {
				"model": "gsr_yatm:block/steel_boiler",
				"y": 90
			}
		},
		{
			"when": {
				"AND": [
					{
						"lit": "false",
						"facing": "south"
					}
				]
			},
			"apply": {
				"model": "gsr_yatm:block/steel_boiler",
				"y": 180
			}
		},
		{
			"when": {
				"AND": [
					{
						"lit": "false",
						"facing": "west"
					}
				]
			},
			"apply": {
				"model": "gsr_yatm:block/steel_boiler",
				"y": 270
			}
		},
		
		
		
		{
			"when": {
				"AND": [
					{
						"lit": "true",
						"facing": "north"
					}
				]
			},
			"apply": {
				"model": "gsr_yatm:block/steel_boiler_lit"
			}
		},
		{
			"when": {
				"AND": [
					{
						"lit": "true",
						"facing": "east"
					}
				]
			},
			"apply": {
				"model": "gsr_yatm:block/steel_boiler_lit",
				"y": 90
			}
		},
		{
			"when": {
				"AND": [
					{
						"lit": "true",
						"facing": "south"
					}
				]
			},
			"apply": {
				"model": "gsr_yatm:block/steel_boiler_lit",
				"y": 180
			}
		},
		{
			"when": {
				"AND": [
					{
						"lit": "true",
						"facing": "west"
					}
				]
			},
			"apply": {
				"model": "gsr_yatm:block/steel_boiler_lit",
				"y": 270
			}
		},
		
		
		
		
		
		{
			"when": {
				"has_tank": "true"
			},
			"apply": {
				"model": "gsr_yatm:block/steel_boiler_has_tank_multipart"
			}
		}
	]
}
